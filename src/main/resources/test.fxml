<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
        <stylesheets>

            <URL value="@viewStyle.css" />
        </stylesheets>
        <children>
            <HBox spacing="10.0" styleClass="container">
                <Label fx:id="panierLabel" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="123.0" text="Label" textFill="#fffdfd" />
                <Label fx:id="enchereLabel" alignment="CENTER" prefHeight="17.0" prefWidth="159.0" text="Label" textFill="#e8e5e5" />
                <Label fx:id="lotLabel" alignment="TOP_LEFT" prefHeight="17.0" prefWidth="67.0" text="Label" textFill="WHITE" />
                <Label fx:id="prixLabel" alignment="CENTER" prefHeight="17.0" prefWidth="127.0" text="Label" textFill="#f5f0f0" />
                <Button fx:id="deleteButton" mnemonicParsing="false" styleClass="form_container Button" stylesheets="@PanierStyle.css" text="Delete" />
                <Button fx:id="updateButton" mnemonicParsing="false" styleClass="action-button" stylesheets="@viewStyle.css" text="Update" />
            </HBox>
        </children>
    </StackPane>
