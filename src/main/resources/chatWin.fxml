<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<VBox stylesheets="@chatCSS.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.CodeCommanders.Controllers.chatWin">

    <TextArea fx:id="zoneDeChat" editable="false" wrapText="true" VBox.vgrow="ALWAYS" />
    <TextField fx:id="messageTextField" promptText="Saisissez votre message..." />
    <Button fx:id="button" onAction="#envoyer" text="Envoyer" />
</VBox>
